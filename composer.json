{
    "name": "samsin33/laravel-razorpay",
    "description": "Laravel razorpay provides support for Laravel with Razorpay.",
    "keywords": ["laravel", "razorpay", "billing"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/samsin33/laravel-razorpay/issues",
        "source": "https://github.com/samsin33/laravel-razorpay"
    },
    "authors": [
        {
            "name": "Samarth Sinha",
            "email": "samarthasinha09@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "illuminate/container": "^7.0|^8.0",
        "illuminate/contracts": "^7.0|^8.0",
        "illuminate/database": "^7.0|^8.0",
        "illuminate/http": "^7.0|^8.0",
        "illuminate/log": "^7.0|^8.0",
        "illuminate/notifications": "^7.0|^8.0",
        "illuminate/routing": "^7.0|^8.0",
        "illuminate/support": "^7.0|^8.0",
        "illuminate/view": "^7.0|^8.0",
        "laminas/laminas-diactoros": "^2.2",
        "moneyphp/money": "^3.2",
        "nesbot/carbon": "^2.0",
        "nyholm/psr7": "^1.0",
        "phpseclib/phpseclib": "^2.0",
        "razorpay/razorpay": "^2.7",
        "symfony/http-kernel": "^4.3|^5.0",
        "symfony/intl": "^4.3|^5.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^4.4|^5.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Samsin33\\Razorpay\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Samsin33\\Razorpay\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Samsin33\\Razorpay\\Providers\\RazorpayServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
